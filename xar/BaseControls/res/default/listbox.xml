		<imagelist id="imagelist.listbox.header.itembkg" vertical="0" splitter="1" varyblock="0" blocksize="7" image="listbox\imagelist.listbox.header.itembkg.png"/>
		<texture id="texture.listbox.header.itembkg.normal" type="ThreeInOneH" imagelist="imagelist.listbox.header.itembkg" index="1"/>
		<texture id="texture.listbox.header.itembkg.hover" type="ThreeInOneH" imagelist="imagelist.listbox.header.itembkg" index="2"/>
		<texture id="texture.listbox.header.itembkg.down" type="ThreeInOneH" imagelist="imagelist.listbox.header.itembkg" index="3"/>
		<!--列表：框选背景-->
		<texture id="texture.tasklist.select" type="NineInOne" image="listbox\texture.tasklist.select.png" />
		
		<texture id="texture.dragitem.bkg" type="NineInOne" image="listbox\dragitem_bkg.png" />
