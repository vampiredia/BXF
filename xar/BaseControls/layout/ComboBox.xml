            <InsertItem file="ComboBox.xml.lua" func="CB__InsertItem" />
            <RemoveItem file="ComboBox.xml.lua" func="CB__RemoveItem" />
            <SelectItem file="ComboBox.xml.lua" func="CB__SelectItem" />
