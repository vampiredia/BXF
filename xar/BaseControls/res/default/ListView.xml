<xlue>
	<resource>
		<!--普通的位图资源-->
		<bitmap id="app.bkg"  path="bitmap\bkg.png" />
		<!--纹理资源-->
		<!--header section background -->
		<imagelist id="imagelist.header.bkg" vertical="0" splitter="1" varyblock="1" image="ListView\header.section.png"/>
		<texture id="section.bkg1" type="ThreeInOneH" imagelist="imagelist.header.bkg" index="1"/>
		<texture id="section.bkg2" type="ThreeInOneH" imagelist="imagelist.header.bkg" index="2"/>
		<texture id="section.bkg3" type="ThreeInOneH" imagelist="imagelist.header.bkg" index="3"/>
		<texture id="section.bkg4" type="ThreeInOneH" imagelist="imagelist.header.bkg" index="4"/>
		<texture id="section.bkg5" type="ThreeInOneH" imagelist="imagelist.header.bkg" index="5"/>
		<texture id="section.bkg6" type="ThreeInOneH" imagelist="imagelist.header.bkg" index="6"/>
		<texture id="section.bkg7" type="ThreeInOneH" imagelist="imagelist.header.bkg" index="7"/>
		<texture id="section.bkg8" type="ThreeInOneH" imagelist="imagelist.header.bkg" index="8"/>
		<texture id="section.bkg9" type="ThreeInOneH" imagelist="imagelist.header.bkg" index="9"/>
		<texture id="section.bkg10" type="ThreeInOneH" imagelist="imagelist.header.bkg" index="10"/>
		<texture id="section.bkg11" type="ThreeInOneH" imagelist="imagelist.header.bkg" index="11"/>
		<texture id="section.bkg12" type="ThreeInOneH" imagelist="imagelist.header.bkg" index="12"/>
		<texture id="section.bkg0" type="ThreeInOneH" imagelist="imagelist.header.bkg" index="0"/>

		<imagelist id="imagelist.list.header.bkg" vertical="0" splitter="1" varyblock="1" image="ListView\list.header.section.png"/>
		<texture id="header.section.bkg1" type="NineInOne" imagelist="imagelist.list.header.bkg" index="1"/>
		<texture id="header.section.bkg2" type="NineInOne" imagelist="imagelist.list.header.bkg" index="2"/>
		<texture id="header.section.bkg3" type="NineInOne" imagelist="imagelist.list.header.bkg" index="3"/>
		<texture id="header.section.bkg4" type="NineInOne" imagelist="imagelist.list.header.bkg" index="4"/>
		<texture id="header.section.bkg5" type="NineInOne" imagelist="imagelist.list.header.bkg" index="5"/>
		<texture id="header.section.bkg6" type="NineInOne" imagelist="imagelist.list.header.bkg" index="6"/>

		<imagelist id="imagelist.list.header1.bkg" vertical="0" splitter="1" varyblock="1" image="ListView\list.header.section1.png"/>
		<texture id="header.section1.bkg1" type="ThreeInOneH" imagelist="imagelist.list.header1.bkg" index="1"/>
		<texture id="header.section1.bkg2" type="ThreeInOneH" imagelist="imagelist.list.header1.bkg" index="2"/>

		<texture id="header.list.view.header" type="ThreeInOneH" image="ListView\list.view.header.png" />
		<!--分割位图得到bitmap-->			
		<imagelist id="bitmap.sysbtn.image" vertical="0" splitter="1" varyblock="0" blocksize="21" image="ListView\system_button.png"/>
		<bitmap id="bitmap.sysbtn.icon.exit" imagelist="bitmap.sysbtn.image" index="7"/>	

		<!-- 滚动条资源 -->
		<imagelist id="imagelist.vscrollbar.button" vertical="0" splitter="1" varyblock="1" image="ListView\scrollbar.vsb.arrow.png"/>
		<texture id="texture.listview.vsb.prebtn.normal" imagelist="imagelist.vscrollbar.button" index="1" />
		<texture id="texture.listview.vsb.prebtn.hover" imagelist="imagelist.vscrollbar.button" index="2" />
		<texture id="texture.listview.vsb.prebtn.down" imagelist="imagelist.vscrollbar.button" index="3" />
		<texture id="texture.listview.vsb.nextbtn.normal" imagelist="imagelist.vscrollbar.button" index="4" />
		<texture id="texture.listview.vsb.nextbtn.hover" imagelist="imagelist.vscrollbar.button" index="5" />
		<texture id="texture.listview.vsb.nextbtn.down" imagelist="imagelist.vscrollbar.button" index="6" />

		<imagelist id="imagelist.vscrollbar.thumb" vertical="0" splitter="1" varyblock="1" image="ListView\scrollbar.vsb.thumb.png"/>
		<texture id="texture.vsb.thumb.normal" type="ThreeInOneV" imagelist="imagelist.vscrollbar.thumb" index="1"/>
		<texture id="texture.vsb.thumb.hover" type="ThreeInOneV" imagelist="imagelist.vscrollbar.thumb" index="2"/>
		<texture id="texture.vsb.thumb.down" type="ThreeInOneV" imagelist="imagelist.vscrollbar.thumb" index="3"/>
		<texture id="texture.vsb.bkg.normal" type="Stretch" image="ListView\scrollbar.vsb.bkg.hover.png"/>
		<texture id="texture.vsb.bkg.hover" type="Stretch" image="ListView\scrollbar.vsb.bkg.hover.png"/>

		<imagelist id="imagelist.hscrollbar.thumb" vertical="0" splitter="1" varyblock="1" image="ListView\scrollbar.hsb.thumb.png"/>
		<texture id="texture.hsb.thumb.normal" type="ThreeInOneH" imagelist="imagelist.hscrollbar.thumb" index="1" />
		<texture id="texture.hsb.thumb.hover" type="ThreeInOneH" imagelist="imagelist.hscrollbar.thumb" index="2" />
		<texture id="texture.hsb.thumb.down" type="ThreeInOneH" imagelist="imagelist.hscrollbar.thumb" index="3" />
		<texture id="texture.hsb.bkg.normal" type="Stretch" ListView="bitmap\scrollbar.hsb.bkg.hover.png"/>
		<texture id="texture.hsb.bkg.hover" type="Stretch" ListView="bitmap\scrollbar.hsb.bkg.hover.png"/>

	</resource>
</xlue>