	<hostwndtemplate id="CoolJ.MainWnd" class="FrameHostWnd" >
		<attr> 
			<title>物管中心</title>
			<layered>1</layered>
			<width>850</width>				<!-- 主窗口宽度 -->
			<height>600</height>			<!-- 主窗口高度 -->
			<visible>1</visible>
			<enable>1</enable>
			<sysmenu>0</sysmenu>
			<appwindow>1</appwindow>
			<enabledrop>1</enabledrop>
			<ellipse>10,10</ellipse>
			<toolwindow>1</toolwindow>
			<border>7,7,7,6</border>
		</attr>
		
		<eventlist>
			<event name="OnSize" file="MainWnd.lua" func="OnSize"/>
			<event name="OnClose" file="MainWnd.lua" func="OnClose" />
		</eventlist>		
	</hostwndtemplate>	
