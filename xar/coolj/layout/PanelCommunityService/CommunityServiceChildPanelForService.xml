<xlue>
	<control class="CoolJ.CommunityServiceChildPanelForService">
		<attr_def>
			<attr name="BorderTexture" type="string">
				<default>texture.tabbody.border</default>
			</attr>
		</attr_def>
		<method_def>
			<Get_CommunityServiceInfo file="CommunityServiceChildPanelForService.xml.lua" func="Get_CommunityServiceInfo"/>
		</method_def>
		<event_def>

		</event_def>
		<objtemplate>
			<children>
				<obj id="bkg" class="TextureObject">
					<attr>
						<left>0</left>
						<top>0</top>
						<width>father.width</width>
						<height>father.height</height>
						<alpha>168</alpha>
					</attr>
				</obj>

				<obj id="ctrl" class="LayoutObject">
					<attr>
						<width>father.width</width>
						<height>father.height</height>
					</attr>
					<children>
						<obj id="text.tip.header" class="TextObject">
							<attr>
								<top>40</top>
								<left>15</left>
								<width>father.width</width>
								<height>18</height>
								<text>选择服务类型</text>
								<font>default.font.bold</font>
								<halign>left</halign>
								<valign>center</valign>
							</attr>
						</obj>
						<obj id="text.tip.sub.header" class="TextObject">
							<attr>
								<top>40</top>
								<left>192</left>
								<width>father.width</width>
								<height>18</height>
								<text>详细信息</text>
								<textcolor>system.blue</textcolor>
								<font>default.font.bold</font>
								<halign>left</halign>
								<valign>center</valign>
							</attr>
						</obj>
			
						<obj id="main.listview" class="ListView">
							<attr>
								<left>10</left>
								<top>60</top>
								<width>170</width>
								<height>father.height-95</height>
								<!-- 以下属性如果用户不设置,会用默认值 -->
								<!-- 以下属性的值就是默认值 -->
								<BackgroundTexture/>
								<ScrollBarClassNameV>ListView.ScrollBarV</ScrollBarClassNameV>
								<ScrollBarClassNameH>ListView.ScrollBarH</ScrollBarClassNameH>
								<ScrollBarSize>20</ScrollBarSize>
								<HeaderClassName>ListView.Header</HeaderClassName>
								<HeaderHeight>30</HeaderHeight>
								<PreloadDataCount>50</PreloadDataCount>
							</attr>
							<eventlist>
								<event name="OnInitControl" file="CommunityServiceChildPanelForService.xml.lua" func="OnInitControl_Main_ListView" />
								<event name="OnDestroy" file="CommunityServiceChildPanelForService.xml.lua" func="OnDestroy_Main_ListView" />
							</eventlist>
						</obj>	
						<obj id="sub.listview" class="ListView">
							<attr>
								<left>190</left>
								<top>60</top>
								<width>509</width>
								<height>father.height-95</height>
								<!-- 以下属性如果用户不设置,会用默认值 -->
								<!-- 以下属性的值就是默认值 -->
								<BackgroundTexture/>
								<ScrollBarClassNameV>ListView.ScrollBarV</ScrollBarClassNameV>
								<ScrollBarClassNameH>ListView.ScrollBarH</ScrollBarClassNameH>
								<ScrollBarSize>20</ScrollBarSize>
								<HeaderClassName>ListView.Header</HeaderClassName>
								<HeaderHeight>30</HeaderHeight>
								<PreloadDataCount>50</PreloadDataCount>
							</attr>
							<eventlist>
								<event name="OnInitControl" file="CommunityServiceChildPanelForService.xml.lua" func="OnInitControl_Sub_ListView" />
								<event name="OnDestroy" file="CommunityServiceChildPanelForService.xml.lua" func="OnDestroy_Sub_ListView" />
							</eventlist>
						</obj>	
						<obj id="btn.main.add" class="WHome.Button">
							<attr>
								<top>father.height - 25</top>
								<left>60</left>
								<width>48</width>
								<height>22</height>
								<zorder>1000</zorder>	
								<Text>添加</Text>
								<NormalBkgID>green.button.normal</NormalBkgID>
								<DownBkgID>green.button.down</DownBkgID>
								<DisableBkgID>green.button.disable</DisableBkgID>
								<HoverBkgID>green.button.hover</HoverBkgID>
								<ShowFocusRect>false</ShowFocusRect>
								<visible>false</visible>
							</attr>
							<eventlist>
								<event name="OnClick" file="CommunityServiceChildPanelForService.xml.lua" func="BTN_Main_Add" />
							</eventlist>
							
						</obj>
						<obj id="btn.sub.add" class="WHome.Button">
							<attr>
								<top>father.height - 25</top>
								<left>450</left>
								<width>48</width>
								<height>22</height>
								<zorder>1000</zorder>	
								<Text>添加</Text>
								<NormalBkgID>green.button.normal</NormalBkgID>
								<DownBkgID>green.button.down</DownBkgID>
								<DisableBkgID>green.button.disable</DisableBkgID>
								<HoverBkgID>green.button.hover</HoverBkgID>
								<ShowFocusRect>false</ShowFocusRect>
								<visible>false</visible>
							</attr>
							<eventlist>
								<event name="OnClick" file="CommunityServiceChildPanelForService.xml.lua" func="BTN_Sub_Add" />
							</eventlist>							
						</obj>								
					</children>
				</obj>
						
			</children>
			<eventlist>
				<event name="OnInitControl" file="CommunityServiceChildPanelForService.xml.lua" func="OnInitControl" />
			</eventlist>
		</objtemplate>
	</control>
</xlue>