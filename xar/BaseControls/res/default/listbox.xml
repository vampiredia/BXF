		<imagelist id="imagelist.hscroll.thumb.bkg" vertical="0" splitter="1" varyblock="0" blocksize="9" image="listbox\imagelist.hscroll.thumb.bkg.png"/>
		<texture id="texture.hscroll.thumb.bkg.normal" type="ThreeInOneH" imagelist="imagelist.hscroll.thumb.bkg" index="1"/>
		<texture id="texture.hscroll.thumb.bkg.hover" type="ThreeInOneH" imagelist="imagelist.hscroll.thumb.bkg" index="2"/>

		<!--列表：表头排序图标-->
		<imagelist id="imagelist.listbox.header.sorticon" vertical="0" splitter="1" varyblock="0" blocksize="7" image="listbox\imagelist.listbox.header.sorticon.png"/>
