                <default>true</default>
            </attr>
            <attr name="NowState" type="int">
                <default>0</default>
            </attr>
            <attr name="ShowList" type="bool">
                <default>false</default>
            </attr>
            <attr name="CanRemove" type="bool">
                <default>false</default>
            </attr>			
			<!--        -->
			<attr name="CanExpand" type="bool">
				<default>true</default>
			</attr>
			<attr name="HighlightSelected" type="bool">
				<default>true</default>
			</attr>
